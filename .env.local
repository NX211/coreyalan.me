# Main configuration - core system settings
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
NEXT_SKIP_STATIC_EXPORT=true

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=local_development_auth_secret_min_32_chars
NEXTAUTH_JWT_SECRET=local_development_jwt_secret_min_32_chars

# Database - PostgreSQL configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/coreyalan_dev?schema=public

# Invoice Ninja
INVOICE_NINJA_URL=https://api.invoiceninja.com/v1
INVOICE_NINJA_API_KEY=your_api_key_here
INVOICE_NINJA_API_SECRET=your_api_secret_here

# OAuth2 Configuration
